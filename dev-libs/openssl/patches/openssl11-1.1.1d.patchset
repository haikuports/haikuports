+    eval /bin/env \
 	 __CNF_CPPDEFINES="'$__CNF_CPPDEFINES'" \
 	 __CNF_CPPINCLUDES="'$__CNF_CPPINCLUDES'" \
 	 __CNF_CPPFLAGS="'$__CNF_CPPFLAGS'" \
