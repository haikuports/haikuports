+#ifdef __HAIKU__
+            extra_size = (NAME_MAX) + path_max() + 1u; // + 1 for "\0"
+#else
             extra_size = (sizeof(dirent) - sizeof(dirent().d_name)) + path_max() + 1u; // + 1 for "\0"
+#endif
         }
     }
 #endif // defined(BOOST_FILESYSTEM_USE_READDIR_R)
