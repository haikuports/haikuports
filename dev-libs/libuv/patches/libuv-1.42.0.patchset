From 6c0ba1aca0e6948ae8f53c66dcaa8c950baba48f Mon Sep 17 00:00:00 2001
From: Jerome Duval <jerome.duval@gmail.com>
Date: Sun, 8 Sep 2019 12:42:17 +0200
Subject: disable setpriority/getpriority for R1B1.


diff --git a/src/unix/core.c b/src/unix/core.c
index 71e9c52..e934f7c 100644
--- a/src/unix/core.c
+++ b/src/unix/core.c
@@ -1420,6 +1420,9 @@ int uv_os_getpriority(uv_pid_t pid, int* priority) {
   if (priority == NULL)
     return UV_EINVAL;
 
+#ifdef __HAIKU__
+  return UV__ERR(ENOSYS);
+#else
   errno = 0;
   r = getpriority(PRIO_PROCESS, (int) pid);
 
@@ -1428,6 +1431,7 @@ int uv_os_getpriority(uv_pid_t pid, int* priority) {
 
   *priority = r;
   return 0;
+#endif
 }
 
 
@@ -1435,8 +1439,10 @@ int uv_os_setpriority(uv_pid_t pid, int priority) {
   if (priority < UV_PRIORITY_HIGHEST || priority > UV_PRIORITY_LOW)
     return UV_EINVAL;
 
+#ifndef __HAIKU__
   if (setpriority(PRIO_PROCESS, (int) pid, priority) != 0)
     return UV__ERR(errno);
+#endif
 
   return 0;
 }
-- 
