ARCHITECTURES="!x86_gcc2 !x86 !x86_64"
if [ "$targetArchitecture" = x86_gcc2 ]; then
SECONDARY_ARCHITECTURES="!x86"
fi

PROVIDES="
	ripgrep$secondaryArchSuffix = $portVersion
	cmd:rg
	"
REQUIRES="
	haiku$secondaryArchSuffix
	"

BUILD_REQUIRES="
	haiku${secondaryArchSuffix}_devel
	lib:libxslt$secondaryArchSuffix
	"
BUILD_PREREQUIRES="
	cmd:a2x
	cmd:cargo$secondaryArchSuffix
	cmd:gcc$secondaryArchSuffix
	"

TEST_REQUIRES="
	devel:libpcre2_posix$secondaryArchSuffix
	"

defineDebugInfoPackage ripgrep$secondaryArchSuffix \
	$binDir/rg

BUILD()
{
	mkdir -p ~/.cargo/registry/cache/github.com-1ecc6299db9ec823
	for i in {2..82}; do
		eval temp=\$sourceDir$i
		cp "$temp"/* ~/.cargo/registry/cache/github.com-1ecc6299db9ec823
	done
	cargo build --release
}

INSTALL()
{
	install -Dm555 target/release/rg $binDir/rg

	install -Dm444 target/release/build/ripgrep-*/out/rg.bash $dataDir/bash-completion/completions/rg
	install -Dm444 complete/_rg $dataDir/zsh/site-functions/_rg

	install -Dm444 target/release/build/ripgrep-*/out/rg.1 $manDir/man1/rg.1
	install -Dm444 -t $docDir *.md
}

TEST()
{
	mkdir -p ~/.cargo/registry/cache/github.com-1ecc6299db9ec823
	for i in \
		{2..11} {13..18} 21 {31..51} {54..56} {58..64} {66..71} {73..82}
	do
		eval temp=\$sourceDir$i
		cp "$temp"/* ~/.cargo/registry/cache/github.com-1ecc6299db9ec823
	done
	cargo test --all
}
