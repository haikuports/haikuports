diff --git a/configure.ac b/configure.ac
index 98a5226..7fc4938 100644
--- a/configure.ac
+++ b/configure.ac
@@ -206,6 +206,7 @@ case $host_os in
      freebsd*) java_os=freebsd ;;
      solaris*) java_os=solaris ;;
      darwin*)  java_os=darwin ;;
+     haiku*)   java_os=haiku ;;
      *)        java_os=$host_os ;;
 esac
 
