BUILD()
{
	autoreconf -fi
	# override CFLAGS to make gcc2 happy by not passing the -g option
	CFLAGS=-O2 runConfigure ./configure
	make $jobArgs
}

INSTALL()
{
	make install

	fixPkgconfig
	prepareInstalledDevelLibs liblept

	packageEntries devel \
		$developDir

	packageEntries tools \
		$binDir
}
