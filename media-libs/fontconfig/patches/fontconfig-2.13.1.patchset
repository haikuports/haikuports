Subject: gcc2 fix


diff --git a/src/fccfg.c b/src/fccfg.c
index d7c48e8..e8bbd50 100644
--- a/src/fccfg.c
+++ b/src/fccfg.c
@@ -2228,6 +2228,7 @@ FcConfigRealFilename (FcConfig		*config,
     const FcChar8 *sysroot = FcConfigGetSysRoot (config);
     FcChar8 *n = FcConfigFilename (url);
     FcChar8 *nn = NULL;
+    FcChar8 *path = FcStrBuildFilename;
 
     if (n)
     {
@@ -2251,7 +2252,7 @@ FcConfigRealFilename (FcConfig		*config,
 		if (!dirname)
