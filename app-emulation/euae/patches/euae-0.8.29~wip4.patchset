-	  UAE_LIBS="$UAE_LIBS $LIBSCG_LIBS"
-        else
-          UAE_LIBS="$UAE_LIBS -L. $SCGLIBS"
-        fi
+        UAE_LIBS="$UAE_LIBS $LIBSCG_LIBS"
       ],
       [
         AC_MSG_RESULT(no)
