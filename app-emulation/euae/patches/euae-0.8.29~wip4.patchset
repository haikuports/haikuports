diff --git a/configure.in b/configure.in
index 12a1f91..945eaf8 100644
--- a/configure.in
+++ b/configure.in
@@ -285,11 +285,11 @@ MDFPP_H=fpp-unknown.h
 case $host_cpu in
      i[[3-6]]86)
        HOST_CPU=i386
