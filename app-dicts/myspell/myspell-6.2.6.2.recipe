{
	true
}

INSTALL()
{
	spellDir=$dataDir/hunspell
	mkdir -p $spellDir
	cp */{af_*,an_*,ar*,be_*,bg_*,bn_*,bo*,br_*,bs_*,dictionaries/ca*,cs_*} $spellDir
	cp */{da_*,de_*,el_*,en_*,es_*,et_*,fr*,gd_*,gl_*,gu_*,gug*} $spellDir
	cp */{he_*,hi_*,hr_*,hu_*,id_*,is*,it_*,kmr_*,lo_*,*nl_*} $spellDir

	packageEntries af \
		$spellDir/af*

	packageEntries an \
		$spellDir/an*

	packageEntries ar \
		$spellDir/ar*

	packageEntries be \
		$spellDir/be*

	packageEntries bg \
