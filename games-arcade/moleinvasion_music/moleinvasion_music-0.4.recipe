ARCHITECTURES="any"
DISABLE_SOURCE_PACKAGE="yes"

PROVIDES="
	moleinvasion_music = $portVersion compat >= 0
	"
REQUIRES="
	moleinvasion == $portVersion compat >= 0
	"

BUILD_REQUIRES="
	"
BUILD_PREREQUIRES="
	cmd:cp
	"

BUILD()
{
	true
}

INSTALL()
{
	mkdir -p $dataDir/MoleInvasion/music
	cp *.ogg $dataDir/MoleInvasion/music
	cp ../../sources*/*.ogg $dataDir/MoleInvasion/music
}
