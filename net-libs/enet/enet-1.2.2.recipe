REVISION="1"
STATUS_HAIKU="stable"
DEPEND=""

BUILD()
{
	cd enet-1.2.2
	libtoolize --force --copy --install
	autoreconf -i
	./configure --prefix=`finddir B_COMMON_DIRECTORY`
}

INSTALL()
{
	cd enet-1.2.2
	make install
}

