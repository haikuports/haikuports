BUILD()
{
	LDFLAGS="-lnetwork -lbsd -lexecinfo" CFLAGS="-D_BSD_SOURCE" \
		make library $jobArgs
}

INSTALL()
{
	make install INSTALL=$prefix

	prepareInstalledDevelLibs libamd \
		libbtf \
		libcamd \
		libccolamd \
		libcholmod \
		libcolamd \
		libcxsparse \
		libgraphblas \
		libklu \
		libldl \
		libmetis \
		libmongoose \
		librbio \
		libspqr \
		libsuitesparseconfig \
		libumfpack

	fixPkgconfig

	mkdir -p $manDir $(dirname $includeDir)
	mv $prefix/include $includeDir
	rm -rf $prefix/share

	# AMD
	packageEntries "amd" \
		"$libDir"/*libamd*
	# BTF
	packageEntries "btf" \
		"$libDir"/*btf*
	# CAMD
	packageEntries "camd" \
		"$libDir"/*libcamd*
	# CCOLAMD
