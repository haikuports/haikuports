+#if defined(__HAIKU__)
+	/*LC_ALL not defined on Haiku*/
+	setenv("LC_ALL","C",1);
+#endif
+
     // required for the button serialization
     // TODO: change to QVector in v1.0
     qRegisterMetaTypeStreamOperators<QList<int> >("QList<int>");
