SOURCE_URI="http://the.earth.li/~sgtatham/putty/latest/putty-0.60.tar.gz"
REVISION="1"
STATUS_HAIKU="broken"
DEPEND=""
CHECKSUM_MD5="07e65fd98b16d115ae38a180bfb242e2"
BUILD()
{
	cd putty-0.60/unix
	autoconf
	./configure --prefix=`finddir B_COMMON_DIRECTORY`
	make all-cli
}

INSTALL()
{
	cd putty-0.60/unix
	make install
}

