-    if test -f $PHP_ZLIB/include/zlib/zlib.h; then
+    if test -f $PHP_ZLIB/develop/headers/zlib/zlib.h; then
       ZLIB_DIR=$PHP_ZLIB
-      ZLIB_INCDIR=$ZLIB_DIR/include/zlib
