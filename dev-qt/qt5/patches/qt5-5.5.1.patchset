+static uint32 translateKeyCode(uint32 key)
+{
+	uint32 code = 0;
+	uint32 i = 0;
+	if(modifiers()&&B_NUM_LOCK) {
+	    while (ScanCodes_Numlock[i]) {
+	      		if ( key == ScanCodes_Numlock[i + 1]) {
+	            code = ScanCodes_Numlock[i];
+	      		    break;
+	        	}
+	       	i += 2;
+	  	}	
+		if(code>0)
+			return code;   
+	}
+	
+	i = 0;
+    while (ScanCodes[i]) {
+      		if ( key == ScanCodes[i + 1]) {
+            code = ScanCodes[i];
+      		    break;
+        	}
+       	i += 2;
+  	}	
+	return code;   
+}
