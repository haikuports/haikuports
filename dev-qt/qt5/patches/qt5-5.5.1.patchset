+{
+	if (fQWindow->window()->windowState() & Qt::WindowMaximized) {
+		QWindowSystemInterface::handleWindowStateChanged(fQWindow->window(), Qt::WindowNoState);
+	}
+	fQWindow->FrameResized(width, height);
