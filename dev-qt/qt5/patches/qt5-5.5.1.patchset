-            GetMouse(&dummyPos, &keyState);
-
-            const Qt::MouseButtons mouseButtons = keyStateToMouseButtons(keyState);
-            const Qt::KeyboardModifiers keyboardModifiers = keyStateToModifiers(modifiers());
