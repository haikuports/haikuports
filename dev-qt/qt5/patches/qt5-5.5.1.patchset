-    if (keyState & B_SHIFT_KEY)
-        modifiers |= Qt::ShiftModifier;
-    if (keyState & B_CONTROL_KEY)
-        modifiers |= Qt::AltModifier;
-    if (keyState & B_COMMAND_KEY)
-        modifiers |= Qt::ControlModifier;
-
-    return modifiers;
